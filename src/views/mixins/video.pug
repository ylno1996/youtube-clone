mixin video(video)
    div     
        h4
            a(href=`/videos/${video.id}`, class="video-small")
                img(src="/uploads/img/thumb.webp", alt="썸네일" , class="thumb")
                span=video.title
        p=video.description
        ul
            each hashtag in video.hashtags
                li=hashtag
        small=video.createdAt
        hr 